{
    "name": "Bigger Theater Mode",
    "version": "0.0.0.4",
    "description": "Increases the size of YouTube's Theater mode.",
    "icons": {
        "16": "icon16.png",
        "32": "icon32.png",
        "64": "icon64.png",
        "128": "icon128.png"
    },
    "content_scripts": [
        {
            "matches": ["https://www.youtube.com/*"],
            "css": ["BiggerTheaterMode.css"]
        }
    ],
    "manifest_version": 3
}
  
